<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="474.0" prefWidth="528.0" styleClass="login_container" stylesheets="@../CSS/Style.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.examen.RegisterController">
   <children>
      <VBox layoutX="1.0" prefHeight="474.0" prefWidth="189.0" styleClass="login_logo_container">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Tomato Enterprise" textAlignment="CENTER" wrappingWidth="188.4677734375">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Text>
         </children>
      </VBox>
      <VBox layoutX="191.0" layoutY="-2.0" prefHeight="442.0" prefWidth="352.0" styleClass="login_form_container">
         <children>
            <Label text="Nom" />
            <TextField id="nomfield" fx:id="nomField" />
            <Label text="Prenom" />
            <TextField id="prenomflied" fx:id="prenomField" />
            <Label prefHeight="17.0" prefWidth="42.0" text="Email" />
            <TextField id="emailField" fx:id="emailField" />
            <Label prefHeight="17.0" prefWidth="68.0" text="Telephone" />
            <TextField id="telfield" fx:id="telField" />
            <Label text="username" />
            <TextField id="usernameField" fx:id="usernameField" />
            <Label text="Password" />
            <TextField id="passwordField" fx:id="passwordField" />
            <Button id="registerButton" fx:id="btnregister" mnemonicParsing="false" onAction="#OnregisterButtonClick" prefHeight="32.0" prefWidth="407.0" text="S'inscrire" />
         </children>
      </VBox>
   </children>
</AnchorPane>
